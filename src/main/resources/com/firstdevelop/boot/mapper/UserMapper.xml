<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.firstdevelop.boot.mapper.UserMapper">
    <select id="selectAll" resultType="com.firstdevelop.boot.entity.UserEntity">
        SELECT id, name, age FROM user
    </select>
    <select id="login" resultType="com.firstdevelop.boot.entity.UserEntity">
    	select * from user where username=#{username}and password =#{password}
    </select>
</mapper>